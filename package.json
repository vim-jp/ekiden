{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "ekiden",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "pnpm '/^.*:check/'",
    "fix": "pnpm '/^.*:fix/'",
    "astro": "astro",
    "astro:check": "astro check",
    "lint:check": "eslint --cache src",
    "lint:fix": "eslint --cache --fix src",
    "test": "vitest",
    "prettier:check": "prettier --cache --check --plugin-search-dir=. src",
    "prettier:fix": "prettier --cache --write --plugin-search-dir=. src",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/rss": "^4.0.13",
    "@astrojs/svelte": "^7.2.1",
    "@fontsource/kaushan-script": "^5.2.8",
    "@fontsource/yuji-syuku": "^5.2.7",
    "@unocss/reset": "^66.5.4",
    "astro": "^5.15.1",
    "chart.js": "^4.5.1",
    "dayjs": "^1.11.19",
    "sharp": "^0.34.5",
    "svelte": "^5.43.5"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.5",
    "@eslint/js": "^9.39.1",
    "@iconify-json/radix-icons": "^1.2.5",
    "@iconify-json/tabler": "^1.2.23",
    "@std/collections": "jsr:^1.0.6",
    "@types/eslint__js": "^9.14.0",
    "@types/node": "^24.10.0",
    "@typescript-eslint/utils": "^8.46.3",
    "@unocss/eslint-config": "^66.5.4",
    "@unocss/preset-web-fonts": "^66.5.4",
    "@unocss/reset": "^66.5.4",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-astro": "^1.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.3.3",
    "type-fest": "^5.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "ufo": "^1.6.1",
    "unocss": "^66.5.5",
    "vitest": "^4.0.6"
  },
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^22.14.0",
      "onFail": "download"
    }
  }
}
